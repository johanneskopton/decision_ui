{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "src/webapp/env.d.ts",
    "src/webapp/**/*",
    "src/webapp/**/*.vue"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/webapp/*"
      ]
    }
  }
}